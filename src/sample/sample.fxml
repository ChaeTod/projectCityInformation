<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="mainGridPane" alignment="center" hgap="10" prefHeight="342.0" prefWidth="520.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane fx:id="mainPane" prefHeight="350.0" prefWidth="520.0">
            <children>
                <Button fx:id="okBtn" disable="true" layoutX="388.0" layoutY="295.0" mnemonicParsing="false" onAction="#getAllInfo" prefHeight="25.0" prefWidth="100.0" text="OK" />
                <ComboBox fx:id="comboBox1" layoutX="14.0" layoutY="107.0" onAction="#showNextSelect" onKeyPressed="#switchToFirstCity" onShowing="#selectCountries" prefWidth="150.0" />
                <Label layoutX="24.0" layoutY="75.0" text="Select the Country">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <Label layoutX="151.0" layoutY="14.0" text="City Information">
                    <font>
                        <Font size="26.0" />
                    </font>
                </Label>
                <Label fx:id="cityLabel" layoutX="227.0" layoutY="75.0" text="Select the City">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <ComboBox fx:id="comboBox2" disable="true" layoutX="202.0" layoutY="107.0" onAction="#showAllLabels" onShowing="#selectCities" prefWidth="150.0" />
                <Label fx:id="weatherLabel" layoutX="263.0" layoutY="175.0" text="Weather" visible="false" />
                <Label fx:id="populationLabel" layoutX="31.0" layoutY="224.0" text="Population" visible="false" />
                <Label fx:id="cityLbl" layoutX="50.0" layoutY="175.0" text="City" visible="false" />
                <Label fx:id="countryLbl" layoutX="38.0" layoutY="201.0" text="Country" visible="false" />
                <Label fx:id="popValueLbl" layoutX="137.0" layoutY="221.0" prefHeight="17.0" prefWidth="45.0" textAlignment="CENTER" />
                <Label fx:id="tempLbl" layoutX="229.0" layoutY="204.0" />
                <Label fx:id="humidityLbl" layoutX="229.0" layoutY="227.0" />
                <Label fx:id="visiblLbl" layoutX="388.0" layoutY="175.0" text="Visibility" visible="false" />
                <Label fx:id="riseLbl" layoutX="388.0" layoutY="202.0" text="Sun rise" visible="false" />
                <Label fx:id="downLbl" layoutX="392.0" layoutY="228.0" text="Sunset" visible="false" />
                <Pane layoutX="386.0" layoutY="58.0" prefHeight="85.0" prefWidth="93.0">
                    <children>
                        <CheckBox fx:id="chkBox1" disable="true" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#setLbl" text="Weather" />
                        <CheckBox fx:id="chkBox2" disable="true" layoutX="14.0" layoutY="54.0" mnemonicParsing="false" onAction="#setLbl" text="City Info" />
                    </children></Pane>
                <Label fx:id="coordLbl" layoutX="48.0" layoutY="260.0" text="Coordinates" visible="false" />
                <Label fx:id="link" layoutX="66.0" layoutY="278.0" text="Label" visible="false" />
                <Button fx:id="coordBtn" layoutX="234.0" layoutY="265.0" mnemonicParsing="false" onAction="#goToCoord" text="Button" visible="false" />
            <Button fx:id="coordBtn" disable="true" layoutX="56.0" layoutY="256.0" mnemonicParsing="false" onAction="#goToCoord" prefHeight="25.0" prefWidth="64.0" text="GO WEB" />
            <Button fx:id="infoBtn" layoutX="461.0" layoutY="21.0" mnemonicParsing="false" onAction="#getInfo" text="Info" />
            <TextField fx:id="srchTxt" layoutX="19.0" layoutY="295.0" onAction="#isTxtEmp" prefHeight="25.0" prefWidth="159.0" />
            <Button fx:id="findBtn" disable="true" layoutX="189.0" layoutY="295.0" mnemonicParsing="false" onAction="#getInfoFromDB" text="Find By" />
            </children></Pane>
    </children>
</GridPane>
